{
  "name": "hichxm/laravel-sortable",
  "description": "Laravel sortable packages for Laravel ecosystem by Hichxm",
  "homepage": "https://github.com/hichxm/laravel-sortable",
  "authors": [
    {
      "name": "Hicham SLIMANI",
      "email": "hicham.slimani@digityourdream.fr"
    }
  ],
  "license": "MIT",
  "require-dev": {
    "orchestra/testbench": "v9.0.3",
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "require": {
    "spatie/laravel-package-tools": "^1.16.4",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "doctrine/dbal": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Hichxm\\LaravelSortable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hichxm\\LaravelSortable\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hichxm\\LaravelSortable\\LaravelSortableServiceProvider"
      ]
    }
  }
}
